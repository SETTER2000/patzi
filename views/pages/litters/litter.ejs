<div id="litter" class="content" v-cloak>

  <el-dialog title="<%= __('Portfolio') %>" :visible.sync="dialogTableVisible">
    <template>
      <el-carousel indicator-position="outside" height="65vh">
        <el-carousel-item v-for="(item, index) of items" :key="index">

          <div class="d-flex justify-content-center align-items-center">

              <img :src="item.src" class="img-fluid height"
                   alt="A generic square placeholder image with rounded corners in a figure.">


          </div>

          <!--<el-image :src="item.src" class="img-fluid" :fit="fits"></el-image>-->

        </el-carousel-item>
      </el-carousel>
    </template>
  </el-dialog>

  <!-- start banner Area -->
  <section class="banner-area relative">
    <div class="container">
      <div class="row d-flex align-items-center justify-content-center">
        <div class="about-content col-lg-12">
          <h1 class="text-white">Litter Details</h1>
          <p>Details about the litter</p>
        </div>
      </div>
    </div>
  </section>
  <!-- End banner Area -->

  <!-- Portfolio Details Area -->
  <section class="portfolio_details_area section-gap">
    <div class="container">
      <div class="portfolio_details_inner">
        <div class="row">
          <div class="col-md-6">
            <div class="left_img">
              <a href="#" @click="showSlider()"><img class="img-fluid" :src="litter.imageSrc" alt=""></a>
            </div>
          </div>

          <div class="offset-md-1 col-md-5">
            <div class="portfolio_right_text mt-30">
              <h4><%= __('Litter') %> {{litter.label.toUpperCase()}}</h4>
              <p v-if="litter.subtitle">{{litter.subtitle}}</p>
              <div class="block mb-5">

                <el-rate
                  v-model="ratio"
                  :colors="colors"
                  @change="changeRatio()"
                  show-score
                  text-color="#ff9900"
                  score-template=" <%= __('points') %> {value} "
                >
                </el-rate>
              </div>

              <ul class="list">
                <li><span><%= __('Name') %>
                    <el-tooltip class="item" effect="dark" content="<%= __('Pre puppy name at birth.') %>"
                                placement="right-start">
                           <i class="el-icon-question"></i>
                   </el-tooltip>
                  </span>: {{litter.title}}
                </li>
                <li><span>Sire</span>: Alfa Laval De Lux Supernova</li>
                <li><span>Dam</span>: Sasquehanna (FCI) Ella</li>
                <li><span>Dob</span>: {{litter.born}}</li>
              </ul>
            </div>
          </div>
        </div>
