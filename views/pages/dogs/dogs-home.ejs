          <div class="d-flex justify-content-center align-items-center">

            <img :src="item.src" class="img-fluid height"
                 alt="A generic square placeholder image with rounded corners in a figure.">


          </div>

          <!--<el-image :src="item.src" class="img-fluid" :fit="fits"></el-image>-->

        </el-carousel-item>
      </el-carousel>
    </template>
  </el-dialog>

