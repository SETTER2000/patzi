<div id="pedigree-home" class="content" v-cloak>

    <!-- Start Work Area Area -->
    <section style="height: 83vh" class="work-area section-gap-top" id="work">
        <div class="container">
            <!--<div class="row">-->
            <!--<div class="col">-->

            <!--<h3><%= __('Born in') %></h3>-->
            <h3 style="margin-bottom: 5px" class="section-title text-center text-uppercase"><%= __('Dog\'s name')
                %></h3>

            <div class="row">
                <div class="col-lg-6 offset-lg-3">
                    <div v-if="me.isSuperAdmin || me.isAdmin" class="text-center">
                        <el-form :model="ruleForm" :rules="rules" ref="ruleForm"
                                 label-width="0"
                                 class="demo-ruleForm">

                            <el-form-item prop="search">
                                <!--:autofocus="true"-->
                                <el-autocomplete
                                        class="inline-input"
                                        prefix-icon="el-icon-search"
                                        style="width: 100%;"
                                        v-model="ruleForm.search"
                                        :fetch-suggestions="querySearchAsync"
                                        clearable
                                        placeholder="<%= __('Start typing the dog\'s name') %>: Poale Ell Adam"
                                        :trigger-on-focus="false"
                                        @select="handleSelect"
                                        @focus="focus"
                                        @change="change"
                                        @blur="blur"
                                >


                                    <!--<el-button type="primary" @click="submitForm('ruleForm')">Create</el-button>-->
                                    <!--<el-button @click="resetForm('ruleForm')">Reset</el-button>-->

                                    <!--<el-button slot="append" @click="submitForm('ruleForm')" icon="el-icon-search"></el-button>-->
                                </el-autocomplete>
                            </el-form-item>
                        </el-form>
                    </div>
                </div>
            </div>
            <div class="d-flex flex-column" v-if="resultSearch">

                <% /* 0 УРОВЕНЬ СОБАКА для которой родословная */ %>
                <div class="d-flex  justify-content-center">
                    <div class="block">
                        <div class="flex justify-content-between align-content-stretch">
                            <h3 style="margin: 0;padding: 0;"><a :href="resultSearch.detail">{{resultSearch
                                    .fullName}}</a></h3>
                            <div style="height: 50px; padding-top: 9px" class="">dob: {{resultSearch.dateBirth |
                                getCreate(me.preferredLocale, 'lll')}}
                            </div>
                        </div>

                        <el-image
                                style="max-width: 470px; "
                                :src="resultSearch.images[0].imageSrc"
                                :preview-src-list="resultSearch.imagesArrUrl"
                        >
                            <div slot="placeholder" class="image-slot">
                                Loading<span class="dot">...</span>
                            </div>
                            <div slot="error" class="image-slot">
                                <i class="el-icon-picture-outline"></i>
                            </div>
                        </el-image>
                    </div>
                </div>

                <% /* 1 УРОВЕНЬ  папа мама */ %>
                <div class="d-flex  justify-content-around" v-if="resultSearch.parents && resultSearch.parents
                        .length > 0">
                    <div class="block" v-for="(o, index) in resultSearch.parents" :key="o.id">
                        <div class="flex justify-content-between align-content-stretch">
                            <h3 style="margin: 0;padding: 0;">{{o.fullName}}</h3>
                            <div style="height: 50px; padding-top: 9px" class="">dob: {{o.dateBirth |
                                getCreate(me
                                .preferredLocale, 'lll')}}
                            </div>
                        </div>
                        <el-image
                                style="max-width: 470px; "
                                :src="o.images[0].imageSrc">
                            <div slot="placeholder" class="image-slot">
                                Loading<span class="dot">...</span>
                            </div>
                            <div slot="error" class="image-slot">
                                <i class="el-icon-picture-outline"></i>
                            </div>
                        </el-image>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
