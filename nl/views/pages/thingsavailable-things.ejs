        <div class="form-group">
          <label>Wie is het?</label>
          <input class="form-control" type="text" v-model="uploadFormData.label" placeholder="Poale Ell Adam">
        </div>
        <div class="form-group">
          <label>Titel</label>
          <input class="form-control" type="text" v-model="uploadFormData.title" placeholder="Brief Information">
        </div>
        <div class="form-group">
          <label>Sub Titel</label>
          <input class="form-control" type="text" v-model="uploadFormData.subtitle"
