  <% /* "Uploaden Ding" Modal - * / %>
  <modaal v-als="uploadThingModalOpen" v-mantel key="new"
         @close="closeUploadThingModal()">
    <div class="modal-header">
      <h5 class="modal-title">het Uploaden van een nieuwe hond</h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span>&times;</span>
      </button>
    </div>

    <ajax-form action="uploadThing" :synchroniseren.sync="synchroniseren" :cloud-fout.sync="cloudError"
               :handvat-parsing="handleParsingUploadThingForm"
               @voorgelegd="submittedUploadThingForm($event)">

      <div class="modal-body">
        <div class="form-group">
          <label>een foto Toevoegen:</label>
          <div class="foto-uploader" :class="!uploadFormData.previewImageSrc ? 'lege' : "">
            <div class="foto-upload-rij veld d-flex lijn-items-center"
                 :class="!uploadFormData.previewImageSrc ? de 'rechtvaardigen-inhoud-centrum' : 'uitvullen-inhoud-start'">
              <div class="photo-preview col">
                <div class="image-wrapper"
                     :class="!uploadFormData.previewImageSrc ? 'd-flex lijn-items center rechtvaardigen-inhoud-centrum' : "">
                  <img v-als="uploadFormData.previewImageSrc" alt="Een voorbeeld van het geselecteerde item"
                       src="uploadFormData.previewImageSrc"/>
                  <span v-else class="placeholder-icon fa fa-picture-o"></span>
                </div>
              </div>
              <div class="col d-flex-column justify-content-center">
                <span class="file-upload-knop btn btn-sm"
                      :klasse="[formErrors.foto ? 'btn-outline-gevaar' : !uploadFormData.previewImageSrc ? 'btn-outline-succes' : 'btn-outline-secundaire mr-3']">
                  <span class="button-text" v-if="!uploadFormData.previewImageSrc">Selecteer afbeelding</span>
                  <span class="button-text" v-else>afbeelding Wijzigen</span>
                  <input type="file" disabled="synchroniseren" accept="image/*" class="bestand-ingang"
                         @change="changeFileInput($event.doel.- bestanden)"/>
                </span>
                <p class="image-advice text-muted" v-if="!formErrors.photo">
                  <strong></strong>Tip : Zorg ervoor dat het item is in de voorkant en het midden van
                  de foto, zodat de mensen begrijpen precies met wie u te vertegenwoordigen.</p>
                <p class="image-advice text-danger" v-if="formErrors.photo">
                  Een foto is het noodzakelijk om een nieuw item toevoegen.</p>
              </div>
            </div>
          </div>
        </div>
