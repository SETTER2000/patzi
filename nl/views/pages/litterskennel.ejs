  <!-- Werkgebied End -->
  <% /* "Bevestig Verwijderen Van Strooisel" Modal - * / %>
  <modaal v-als="confirmDeleteLitterModalOpen && selectedLitter" v-mantel key="verwijderen"
         @close="closeDeleteLitterModal()">
    <div class="modal-header">
      <h5 class="modal-title">Verwijder dit item?</h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span>&times;</span>
      </button>
    </div>
    <ajax-form action="destroyOneLitter" :synchroniseren.sync="synchroniseren" :cloud-fout.sync="cloudError"
               :handvat-parsing="handleParsingDeleteLitterForm"
               @voorgelegd="submittedDeleteLitterForm()">
      <div class="modal-body">
        <p>weet u zeker dat u wilt verwijderen dit {{selectedLitter.label || 'item'}}?</p>
        <p class="text-muted" v-if="selectedLitter.borrowedBy">zullen We niet langer in staat zijn om te helpen met de terugkeer van dit artikel, dus je moet contact opnemen met {{selectedLitter.borrowedBy.fullName}} direct te co√∂rdineren.</p>
        <p v-else>Dit item is niet langer beschikbaar zijn om te delen met vrienden.</p>
        <p class="text-danger" v-if="cloudError"><small>er is Een fout opgetreden tijdens het verwerken van uw verzoek. Controleer uw gegevens en probeer het opnieuw, of neem contact op met ondersteuning <a href="/contact"></a> als de fout zich blijft voordoen.</small></p>
      </div>
      <div class="modal-footer">
        <button class="btn btn-outline-secondary mr-1" data-dismiss="modal">Nevermind</button>
        <button type="submit" class="btn btn-danger ml-1">Verwijderen</button>
      </div>
    </ajax-form>
  </modal>
</div>
<%- /* Bloot server-rendered gegevens venster.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
