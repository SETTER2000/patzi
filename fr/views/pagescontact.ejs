              <div class="icon"><i class="ti-home"></i></div>
              <div class="contact-details">
                <h5><%= __('Moscow, Russia') %></h5>
                <p><%= __('st. Ostrovityanova d. 27 Bldg. 3') %></p>
              </div>
            </div>
            <div class="p-2 flex-fill bd-highlight">
              <div class="icon"><i class="ti-headphone"></i></div>
              <div class="contact-details">
                <h5>07 (910) 4067 406</h5>
                <p><%= __('Mon to Mon 9am to 9pm') %></p>
              </div>
            </div>
            <div class="p-2 flex-fill bd-highlight">
              <div class="icon"><i class="ti-email"></i></div>
              <div class="contact-details">
                <!--<h5>support@poaleell.com</h5>-->
                <p><a href="#feedback"><%= __('Send us your query anytime!') %></a></p>
              </div>
            </div>

        <!--  <div class="single-contact-address d-flex flex-row">

          </div>-->
        </div>

