<div id="contact" v-cloak>

    <patzi-section-header :obj-data="seo"></patzi-section-header>
    <!-- Start Service Area -->

    <!-- End Service Area -->
    <section class="service-area section-gap-top">
        <div class="container">
            <patzi-row-h2-h3 :obj-data="seo" :hidden-h3="!cloudSuccess"></patzi-row-h2-h3>

            <div class="row">
                <div class="col-lg-12">
                    <div class="contact-form text-left" v-if="!cloudSuccess">
                        <p class="text-center"><%= __('Maybe some feedback?  We love talking to users about Poale Ell, and we\'re happy to answer questions about our pricing, roadmap, or business solutions.  Send us a note and we\'ll get back to you as soon as possible.') %></p>
                        <hr/>
                        <ajax-form action="deliverContactFormMessage" :syncing.sync="syncing"
                                   :cloud-error.sync="cloudError" @submitted="submittedForm()"
                                   :handle-parsing="handleParsingForm">
                            <div class="form-group">
                                <label for="full-name"><%= __('Name') %></label>
                                <input class="form-control" id="full-name" name="full-name" type="text"
                                       :class="[formErrors.fullName ? 'is-invalid' : '']"
                                       v-model.trim="formData.fullName" placeholder="<%= __('Sturgis P. Sturgeon') %>"
                                       autocomplete="name" autofocus>
                                <div class="invalid-feedback" v-if="formErrors.fullName">Please let us know what to call
                                    you.
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="email-address"><%= __('Email address') %></label>
                                <input class="form-control" id="email-address" name="email-address" type="email"
                                       :class="[formErrors.emailAddress ? 'is-invalid' : '']"
                                       v-model.trim="formData.emailAddress"
                                       placeholder="<%= __('your email, example: info@example.com') %>"
                                       autocomplete="email">
                                <div class="invalid-feedback"
                                     v-if="formErrors.emailAddress"><%= __('Please enter a valid email address') %>.
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="topic"><%= __('Topic') %></label>
                                <input class="form-control" id="top" name="topic" type="text"
                                       :class="[formErrors.topic ? 'is-invalid' : '']" v-model.trim="formData.topic"
                                       placeholder="<%= __('Buying a dog') %>" autocomplete="none">
                                <div class="invalid-feedback"
                                     v-if="formErrors.topic"><%= __('Please choose a topic for your message') %>.
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="message"><%= __('Message') %></label>
                                <textarea class="form-control" id="message" name="message"
                                          :class="[formErrors.message ? 'is-invalid' : '']"
                                          v-model.trim="formData.message"
                                          placeholder="<%= __('What is the difference between a "Hairless" and "Powderpuff" dog?') %>"
                                          autocomplete="none"></textarea>
                                <div class="invalid-feedback"
                                     v-if="formErrors.message"><%= __('Message cannot be empty') %>.
                                </div>
                            </div>
                            <p class="text-danger" v-if="cloudError">
                                <small><%= __('An error occured while processing your request. Please check your information and try again, or') %>
                                    <a href="/contact"><%= __('contact support') %></a> <%= __('if the error persists') %>
                                    .</small></p>
                            <div class="form-group">
                                <ajax-button type="submit" :syncing="syncing"
                                             class="btn btn-dark btn-lg btn-block"><%= __('Send message') %></ajax-button>
                            </div>
                        </ajax-form>
                    </div>
                    <div class="success-message" v-if="cloudSuccess">
                        <p class="text-center"><%= __('Thanks for reaching out') %>!</p>
                        <hr/>
                        <p><%= __('We have received your message, and someone from our team will get back to you soon') %>
                            .</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Start contact-page Area -->
    <section class="service-area ">
        <h2 class="text-center"><%= __('Where to find us') %>?</h2>
        <div class="contact-form"><p
                    class="text-center"><%= __('The nursery is located in Russia in the South-West of Moscow, Konkovo district, near the park Troparyovo. Green, clean, a great place to walk with dogs in Moscow. Come on.') %></p>
        </div>
        <div class="container">

            <div class="row">
                <div class="col-lg-12">
                    <hr>
                    <div
                            class="map-wrap"
                            style="width:100%; height: 445px;"
                            id="map"
                    ></div>
                </div>

                <div class="d-flex align-content-stretch flex-wrap">
                    <!--  <div class="single-contact-address d-flex flex-row">

                      </div>-->

                    <div class="p-2 flex-fill bd-highlight">
                        <div class="icon"><i class="ti-home"></i></div>
                        <div class="contact-details">
                            <h5><%= __('Moscow, Russia') %></h5>
                            <p><%= __('st. Ostrovityanova d. 27 Bldg. 3') %></p>
                        </div>
                    </div>
                    <div class="p-2 flex-fill bd-highlight">
                        <div class="icon"><i class="ti-headphone"></i></div>
                        <div class="contact-details">
                            <h5>07 (910) 4067 406</h5>
                            <p><%= __('Mon to Mon 9am to 9pm') %></p>
                        </div>
                    </div>
                    <div class="p-2 flex-fill bd-highlight">
                        <div class="icon"><i class="ti-email"></i></div>
                        <div class="contact-details">
                            <!--<h5>support@poaleell.com</h5>-->
                            <p><a href="#feedback"><%= __('Send us your query anytime!') %></a></p>
                        </div>
                    </div>

                    <!--  <div class="single-contact-address d-flex flex-row">

                      </div>-->
                </div>

