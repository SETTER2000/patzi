      <% /* ФОТО ПИТОМНИКОВ */ %>
      <div class="scrollbar" id="style-1">
        <el-row :gutter="2" class="flex-container wrap">
          <el-col v-if="kennel.images[kennel.cover] && kennel.see" class="flex-item"
                  v-for="(kennel, index) in kennels.filter(kennel=>kennel.see)" :key="kennel.id"
                  :xs="24" :sm="24" :md="24" :lg="8" :xl="8">

            <div @mouseenter="showMenu(kennel.id)" @mouseleave="showOut()"
                 style="position: relative;" class="ell-image-flex"
                 v-if="!_.isEmpty(kennel.imagesArrUrl)">
              <transition-group appear name="el-zoom-in-top">
                <div v-show="show && (showDog === kennel.id)" class="transition-box" :key="kennel.id">
                  <% /* Кнопки управления собакой */ %>
                  <div class="menu-right mt-1">
                    <el-link type="danger" class="px-2" @click="goTo2(kennel.detail)" :underline="false"
                             title="<%= __('More') %>">
                      {{kennel.fullName}}
                    </el-link>
                    &lt;!&ndash;<el-link type="danger" class="px-2" @click="openRemoveDialog(kennel.id)" :underline="false"
                             title="<%= __('Remove') %>"
                             icon="el-icon-close"></el-link>&ndash;&gt;
                  </div>
                </div>
              </transition-group>
              <el-image
                :src="kennel.images[kennel.cover].imageSrc"
                :key="kennel.id"
                :fit="'cover'"
                :scroll-container="'#style-1'"
                lazy
                :preview-src-list="kennel.imagesArrUrl">
              </el-image>
            </div>
          </el-col>
        </el-row>
      </div>
    </div>
  </section>

</div>

<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
