              &lt;!&ndash;<%= __('Add presentation') %>&ndash;&gt;
              &lt;!&ndash;</el-dropdown-item>&ndash;&gt;
              &lt;!&ndash;<el-dropdown-item icon="el-icon-edit" :command="{com:'a',i:photoSet.indexPhotoSet}">&ndash;&gt;
              &lt;!&ndash;<%= __('Photo session name') %>&ndash;&gt;
              &lt;!&ndash;</el-dropdown-item>&ndash;&gt;
              &lt;!&ndash;<el-dropdown-item icon="el-icon-edit" :command="{com:'b',i:photoSet.indexPhotoSet}">&ndash;&gt;
              &lt;!&ndash;<%= __('Photo session description') %>&ndash;&gt;
              &lt;!&ndash;</el-dropdown-item>&ndash;&gt;

