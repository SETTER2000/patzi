<div id="blog-home" v-cloak>
    <% /* Диалоговое окно. Добавить | Редактировать пост. */ %>
    <el-dialog title="<%= __('Add post') %>"
               :visible.sync="centerDialogAdded"
               custom-class="dialog-window"
               center>
        <el-dialog
                width="60%"
                :title="dialog.header"
                :visible.sync="innerVisible"
                append-to-body>
            <div class="text-center"><p>{{dialog.description}}</p></div>
            <el-image :src="dialog.src" style="width: 100%">
                <div slot="placeholder" class="image-slot">
                    Loading<span class="dot">...</span>
                </div>

            </el-image>
        </el-dialog>
        <% /* Диалоговое окно. Не верный текст. */ %>
        <el-dialog
                title="<%= __('Error') %>"
                width="30%"
                :visible.sync="ruleForm.errInputDogName"
                :show-close=false
                append-to-body>
            <span><%= __('The name of the dog is written only in English letters. Switch the keyboard to the correct layout.') %></span>
            <span slot="footer" class="dialog-footer">
    <el-button autofocus type="primary" @click="ruleForm.errInputDogName = false; ruleForm.label=''">Ok</el-button>
  </span>
        </el-dialog>
        <div class="d-flex justify-content-center">
            <template v-if="!ruleForm.see">
                <el-popover
                        ref="popover"
                        placement="top"
                        title="<%= __('Visibility') %>"
                        width="270"
                        trigger="hover"
                        content="<%= __('Your post is not currently visible on the blog. Until you turn on visibility, no one can read it.') %>">
                </el-popover>
                <div class="menu-center">
                    <h1 v-if="buttonUpdate" class="flex justify-content-center"
                        :class="classObject"
                    >{{ruleForm.fullName}}
                    </h1>
                    <el-button class="not-border" icon="el-icon-warning-outline" size="mini" v-popover:popover
                               circle></el-button>
                </div>
            </template>
            <template v-else>
                <h1 v-if="buttonUpdate" class="flex justify-content-center"
                    :class="classObject"
                >{{ruleForm.fullName}}</h1>
            </template>
        </div>
        <div v-if="ruleForm.imageUrl">
            <div class="single-gallery-image" :style="{ backgroundImage: `url('${ruleForm.imageUrl}')` }"></div>
        </div>
        <h5><%= __('Multiple upload photos') %></h5>
        <label v-if="!ruleForm.imageUrl">
            <%= __('The first photo in this download will become the album cover. Later in the editor you can change these settings.') %>
        </label>
        <div class="my-2">
            <el-divider content-position="left"><%= __('Add photo') %></el-divider>
        </div>
        <!---------------------------------------------------------------------------------------->
        <% /*Загрузка фото NEW*/ %>
        <div class="container-fluid ">
            <div class="row justify-content-center">
                <div class="col">
                    <el-upload
                            action="/api/v1/files/upload"
                            list-type="picture-card"
                            drag
                            multiple
                            ref="upload"
                            :on-preview="handlePictureCardPreview"
                            :on-success="handleSuccess"
                            :on-remove="handleRemove"
                    >
                        <i class="el-icon-plus"></i>
                        <div class="el-upload__tip" slot="tip"><%= __('jpg/png files with a size less than ') %>
                            {{sizeLess}}<%= __('MB') %></div>
                    </el-upload>
                    <br>
                    <el-dialog :visible.sync="dialogVisible" style="z-index: 10000">
                        <img :src="dialogImageUrl" alt="">
                    </el-dialog>
                </div>
            </div>
        </div>
        <!---------------------------------------------------------------------------------------->
        <el-form :model="ruleForm" :rules="rules" ref="ruleForm" class="demo-ruleForm ">
            <div class="d-flex justify-content-around">
            <div style="width: 41vw; border-right:0.1em solid #e9e9e9  ">

                <% /* ВИДЕН ПОСТ ИЛИ НЕТ */ %>
                <el-form-item label="<%= __('Article') %>" prop="see">
                    <el-row :gutter="2">
                        <el-col :span="5">
                            <el-switch
                                    style="display: block; padding-bottom: 0"
                                    v-model="ruleForm.see"
                                    active-color="#13ce66"
                                    inactive-color="#ff4949"
                                    active-text="<%= __('visible') %>"
                                    inactive-text="<%= __('not visible') %>">
                            </el-switch>
                        </el-col>
                        <el-col :span="5">
                            <el-tooltip effect="dark"
                                        content="<%= __("Will your article/post on the site be visible?") %>"
                                        placement="top-start">

                                <div size="mini" round class="el-icon-question "></div>
                            </el-tooltip>
                        </el-col>
                    </el-row>
                </el-form-item>

                <% /* НА ГЛАВНУЮ */ %>
                <el-form-item label="<%= __('On the main') %>" prop="rootPage">
                    <el-row :gutter="2">
                        <el-col :span="4">
                            <el-switch
                                    style="display: block"
                                    v-model="ruleForm.rootPage"
                                    active-color="#13ce66"
                                    inactive-color="#ff4949"
                                    active-text="<%= __('on') %>"
                                    inactive-text="<%= __('off') %>">
                            </el-switch>
                        </el-col>
                        <el-col :span="5">
                            <el-tooltip effect="dark"
                                        content="<%= __("Enable the display of this topic on the main page of the site?") %>"
                                        placement="top-start">

                                <div size="mini" round class="el-icon-question "></div>
                            </el-tooltip>
                        </el-col>
                    </el-row>
                </el-form-item>

                <% /* ТЕМА */ %>
                <el-form-item v-if="!buttonUpdate" label="<%= __('Topic') %>" prop="topic">
                    <el-select v-model="ruleForm.topic" placeholder="<%= __('Choose your cattery') %>"
                               class="form-width"
                    >
                        <el-option v-for="item in getPull()"
                                   :key="item.id"
                                   :label="item.labelRu"
                                   :value="item.id">
                            <div class="d-flex justify-content-between">
                                <div class="size pr-5">{{ item.labelRu | trimString}}</div>
                                <div class="size">{{ item.label | trimString}}</div>
                            </div>
                        </el-option>
                    </el-select>
                    <el-tooltip effect="dark"
                                content="<%= __("Add topic") %>"
                                placement="top-start">
                        <el-button @click="goTo('topics')" size="mini" round icon="el-icon-plus"></el-button>
                    </el-tooltip>
                    <el-tooltip effect="dark"
                                content="<%= __("If you have not found your cattery, go to the \"topics\" section and create it.") %>"
                                placement="top-start">

                        <div size="mini" round class="el-icon-question "></div>
                    </el-tooltip>
                </el-form-item>

                <% /* ЭКСПЕРТЫ  */ %>
                <el-form-item v-if="!buttonUpdate" label="<%= __('Expert') %>" prop="experts">
                    <el-select v-model="ruleForm.experts"
                               placeholder="<%= __('Choose your expert') %>"
                               class="form-width"
                               multiple
                    >
                        <el-option v-for="item in getPullExpert()"
                                   :key="item.id"
                                   :label="me.preferredLocale === 'ru' ? item.fullName : item.fullNameEn"
                                   :value="item.id">
                            <div class="d-flex justify-content-between">
                                <div class="size pr-5">{{ item.fullName}}</div>
                                <div class="size">{{ item.fullNameEn }}</div>
                            </div>
                        </el-option>
                    </el-select>
                    <el-tooltip effect="dark"     v-if="me.isAdmin || me.isSuperAdmin"
                                content="<%= __("Add expert") %>"
                                placement="top-start">
                        <el-button @click="goTo('users')" size="mini" round icon="el-icon-plus"></el-button>
                    </el-tooltip>
                    <el-tooltip effect="dark"
                                content="<%= __("If you haven't found the expert you are looking for, go to the Users section and create one.") %>"
                                placement="top-start">

                        <div size="mini" round class="el-icon-question "></div>
                    </el-tooltip>
                </el-form-item>

                <% /* ДАТА */ %>
                <el-form-item v-if="!buttonUpdate" label="<%= __('Date') %>" prop="dateEvent">
                    <el-row :gutter="2">
                        <el-col :span="11">
                            <el-date-picker
                                    v-model="ruleForm.dateEvent"
                                    type="datetime"
                                    placeholder="<%= __('Select date and time') %>"
                                    default-time="12:00:00">
                            </el-date-picker>
                        </el-col>
                    </el-row>
                </el-form-item>
                <el-form-item v-if="buttonUpdate" label="<%= __('Date') %>" prop="dateEventUpdate">
                    <el-row :gutter="2">
                        <el-col :span="11">
                            <el-date-picker
                                    v-model="dateEventUpdate"
                                    type="datetime"
                                    placeholder="<%= __('Select date and time') %>"
                                    default-time="12:00:00">
                            </el-date-picker>
                        </el-col>
                    </el-row>
                </el-form-item>

                <% /* НАЗВАНИЕ ПОСТА на русском*/ %>
                <template>
                    <el-form-item label="<%= __('Topic Title') %> (Ru)" prop="labelRu">
                        <el-input show-word-limit
                                  style="width:80%;"
                                  clearable
                                  maxlength="120"
                                  size="small"
                                  :keyup="ruleForm.labelRu | abc(ruleForm,'labelRu','ru')"
                                  placeholder="<%= __('Topic Title') %>"
                                  v-model="ruleForm.labelRu"></el-input>
                    </el-form-item>
                </template>
                <% /* ОПИСАНИЕ ПОСТА на русском */ %>
                <template>
                    <el-form-item label="<%= __('Description') %> (Ru)" prop="subtitleRu">
                        <el-col :xs="16" :sm="15" :md="15" :lg="14" :xl="14">
                            <el-input
                                    type="textarea"
                                    :rows="3"
                                    clearable
                                    autosize
                                    :keyup="ruleForm.subtitleRu "
                                    placeholder="Please input"
                                    v-model="ruleForm.subtitleRu"
                                    maxlength="2000"
                                    show-word-limit>
                            </el-input>
                        </el-col>
                    </el-form-item>
                </template>
                <% /* НАЗВАНИЕ ПОСТА на английском*/ %>
                <template>
                    <el-form-item label="<%= __('Topic Title') %> (En)" prop="label">
                        <el-input show-word-limit
                                  style="width:80%;"
                                  clearable
                                  maxlength="120"
                                  size="small"
                                  :keyup="ruleForm.label | abc(ruleForm,'label','en')"
                                  placeholder="<%= __('Topic Title') %>"
                                  v-model="ruleForm.label"></el-input>
                    </el-form-item>
                </template>
                <% /* ОПИСАНИЕ ПОСТА на английском */ %>
                <template>
                    <el-form-item label="<%= __('Description') %> (En)" prop="subtitle">
                        <el-col :xs="16" :sm="15" :md="15" :lg="14" :xl="14">
                            <el-input
                                    type="textarea"
                                    :rows="3"
                                    clearable
                                    autosize
                                    :keyup="ruleForm.subtitle "
                                    placeholder="Please input"
                                    v-model="ruleForm.subtitle "
                                    maxlength="2000"
                                    show-word-limit>
                            </el-input>
                        </el-col>
                    </el-form-item>
                </template>

            </div>
            <div style="width: 40vw;">

                <% /* ВИДЕО URL */ %>
                <el-form-item v-if="!buttonUpdate" label="<%= __('Url youtube') %>" prop="videoUrl">
                    <el-input show-word-limit
                              style="width:60%;"
                              clearable
                              maxlength="700"
                              size="small"
                              placeholder="https://youtu.be/TUar0_aUWq8"
                              v-model="ruleForm.videoUrl"></el-input>
                    <el-tooltip effect="dark"
                                content="<%= __("Insert part of the url from the video here youtube 'https://youtu.be/TUar0_aUWq8'") %>"
                                placement="top-start">

                        <div size="mini" round class="el-icon-question "></div>
                    </el-tooltip>
                </el-form-item>

                <% /* ВИДЕО Заголовок */ %>
                <el-form-item v-if="!buttonUpdate" label="<%= __('Title video') %>" prop="videoHeader">
                    <el-input show-word-limit
                              style="width:60%;"
                              clearable
                              maxlength="200"
                              size="small"
                              placeholder="Title"
                              v-model="ruleForm.videoHeader"></el-input>
                    <el-tooltip effect="dark"
                                content="<%= __("The title for the video is displayed on the post page") %>"
                                placement="top-start">

                        <div size="mini" round class="el-icon-question "></div>
                    </el-tooltip>
                </el-form-item>

                <% /* ВИДЕО Описание */ %>
                <el-form-item v-if="!buttonUpdate" label="<%= __('Description video') %>" prop="videoDescription">
                    <el-input show-word-limit
                              style="width:60%;"
                              clearable
                              maxlength="700"
                              size="small"
                              placeholder="Description"
                              v-model="ruleForm.videoDescription"></el-input>
                    <el-tooltip effect="dark"
                                content="<%= __("The description for the video is displayed on the post page") %>"
                                placement="top-start">
                        <div size="mini" round class="el-icon-question "></div>
                    </el-tooltip>
                </el-form-item>

                <% /* ВИДЕО Showinfo:  */ %>
                <%= __('Showinfo youtube') %>
                <template>
                    <el-row :gutter="2">
                        <el-form-item prop="videoShowinfo">
                            <el-col :xs="12" :sm="15" :md="14" :lg="13" :xl="12">
                                <el-slider
                                        class="hidden-xs-only"
                                        v-model="ruleForm.videoShowinfo"
                                        class="hidden-xs-only"
                                        :min="0"
                                        :max="400"
                                        :step="1"
                                        show-input>
                                </el-slider>
                                <el-input-number :min="0" class="text-center hidden-sm-and-up"
                                                 :max="400" size="mini" v-model="ruleForm.videoShowinfo"></el-input-number>
                            </el-col>
                            <el-col :xs="3" :sm="3" :md="3" :lg="3" :xl="2">
                                <el-tooltip effect="dark"
                                            content="<%= __("Specify the weight at birth.") %>"
                                            placement="top-start">
                                    <div size="mini" round class="el-icon-question ml-3"></div>
                                </el-tooltip>
                            </el-col>
                        </el-form-item>
                    </el-row>
                </template>

                <% /* ВИДЕО videoStart:  */ %>
                <%= __('Start youtube') %>
                <template>
                    <el-row :gutter="2">
                        <el-form-item prop="videoStart">
                            <el-col :xs="12" :sm="15" :md="14" :lg="13" :xl="12">
                                <el-slider
                                        class="hidden-xs-only"
                                        v-model="ruleForm.videoStart"
                                        class="hidden-xs-only"
                                        :min="0"
                                        :max="400"
                                        :step="1"
                                        show-input>
                                </el-slider>
                                <el-input-number :min="0" class="text-center hidden-sm-and-up"
                                                 :max="400" size="mini" v-model="ruleForm.videoStart"></el-input-number>
                            </el-col>
                            <el-col :xs="3" :sm="3" :md="3" :lg="3" :xl="2">
                                <el-tooltip effect="dark"
                                            content="<%= __("Specify the weight at birth.") %>"
                                            placement="top-start">
                                    <div size="mini" round class="el-icon-question ml-3"></div>
                                </el-tooltip>
                            </el-col>
                        </el-form-item>
                    </el-row>
                </template>

                <% /* ВИДЕО videoControls:  */ %>
                <%= __('Controls youtube') %>
                <template>
                    <el-row :gutter="2">
                        <el-form-item prop="videoControls">
                            <el-col :xs="12" :sm="15" :md="14" :lg="13" :xl="12">
                                <el-slider
                                        class="hidden-xs-only"
                                        v-model="ruleForm.videoControls"
                                        class="hidden-xs-only"
                                        :min="0"
                                        :max="400"
                                        :step="1"
                                        show-input>
                                </el-slider>
                                <el-input-number :min="0" class="text-center hidden-sm-and-up"
                                                 :max="400" size="mini" v-model="ruleForm.videoControls"></el-input-number>
                            </el-col>
                            <el-col :xs="3" :sm="3" :md="3" :lg="3" :xl="2">
                                <el-tooltip effect="dark"
                                            content="<%= __("Specify the weight at birth.") %>"
                                            placement="top-start">
                                    <div size="mini" round class="el-icon-question ml-3"></div>
                                </el-tooltip>
                            </el-col>
                        </el-form-item>
                    </el-row>
                </template>

                <% /* ВИДЕО videoMute:  */ %>
                <%= __('Mute youtube') %>
                <template>
                    <el-row :gutter="2">
                        <el-form-item prop="videoMute">
                            <el-col :xs="12" :sm="15" :md="14" :lg="13" :xl="12">
                                <el-slider
                                        class="hidden-xs-only"
                                        v-model="ruleForm.videoMute"
                                        class="hidden-xs-only"
                                        :min="0"
                                        :max="400"
                                        :step="1"
                                        show-input>
                                </el-slider>
                                <el-input-number :min="0" class="text-center hidden-sm-and-up"
                                                 :max="400" size="mini" v-model="ruleForm.videoMute"></el-input-number>
                            </el-col>
                            <el-col :xs="3" :sm="3" :md="3" :lg="3" :xl="2">
                                <el-tooltip effect="dark"
                                            content="<%= __("Specify the weight at birth.") %>"
                                            placement="top-start">
                                    <div size="mini" round class="el-icon-question ml-3"></div>
                                </el-tooltip>
                            </el-col>
                        </el-form-item>
                    </el-row>
                </template>


            </div>
            </div>
            <el-divider></el-divider>

            <div class="d-flex justify-content-center">
                <div class="my-3 pt-4 text-center hidden-xs-only">
                    <el-form-item>

                        <el-button v-if="!buttonUpdate"
                                   @click="resetForm('ruleForm')"><%= __('Reset') %></el-button>
                        <el-button
                                @click="buttonUpdate = false; centerDialogAdded = false"><%= __('Cancel') %></el-button>
                        <el-button v-if="!buttonUpdate" type="primary"
                                   @click="submitForm('ruleForm')"><%= __('Create') %></el-button>
                        <el-button v-else type="primary"
                                   @click="submitForm('ruleForm')"><%= __('Update') %></el-button>

                    </el-form-item>
                </div>
                <div class="text-center hidden-sm-and-up">
                    <el-button-group>
                        <el-button v-if="!buttonUpdate" @click="resetForm('ruleForm')"
                                   icon="el-icon-minus"></el-button>
                        <el-button @click="buttonUpdate = false; centerDialogAdded = false"
                                   icon="el-icon-close"></el-button>
                        <el-button v-if="!buttonUpdate" type="primary"
                                   @click="submitForm('ruleForm')" icon="el-icon-check"></el-button>
                        <el-button v-else type="primary" @click="submitForm('ruleForm')"
                                   icon="el-icon-check"></el-button>
                    </el-button-group>
                </div>
            </div>
        </el-form>
        <span slot="footer" class="dialog-footer">
  </span>
    </el-dialog>
    <% /* Диалоговое окно. Подсказка о создание темы. */ %>
    <el-dialog
            :visible.sync="centerDialogVisible"
            custom-class="dialog-kennel-add"
            center>
        <!-- Start Pricing Area -->
        <section>
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title text-center">
                        <h3><%= __('Need to do now') %>!</h3>
                        <h2 class="mt-5"><span><%= __('Post Topics') %></span></h2>
                        <p>
                        <ul>
                            <li>
                                <span class=" cross mx-2 text-danger"></span><%= __('Your system currently does not have a blog topic catalog. Any blog post should be tied to a specific topic. You need to create at least one topic in the Themes module. To do this, go to the main menu Settings -> Themes') %>
                                <el-link href="/topics" icon="el-icon-top-right"></el-link>
                            </li>
                        </ul>
                        </p>
                    </div>
                </div>
            </div>
        </section>
        <div slot="footer" class="dialog-footer mb-3 pb-5">

            <el-button @click="centerDialogVisible = false"><%= __('Ok') %></el-button>
            <!--<el-button type="info" @click="goTo('account')" size="large"><%= __('Add card') %></el-button>-->
        </div>
    </el-dialog>

    <% /* HEADER */ %>
