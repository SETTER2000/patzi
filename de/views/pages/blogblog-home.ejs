                                <div class="u-container-layout u-valign-middle-xs u-container-layout-1">
                                    <div alt=""
                                         class="u-border-5 u-border-custom-color-6 u-image u-image-circle u-image-1"
                                         data-image-width="1155" data-image-height="650"></div>
                                </div>
