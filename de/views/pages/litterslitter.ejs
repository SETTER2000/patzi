  <% /* Диалоговое окно Слайдер картинок */ %>
  <el-dialog
    title="<%= __('Parents') %>"
    :visible.sync="dialogTableVisible"
    width="60%"
    center>
    <div class="d-flex justify-content-end">
      <el-switch
        style="display: block"
        v-model="autoplay"
        active-color="#13ce66"
        inactive-color="#ff4949"
        active-text="Autoplay"
        inactive-text="Disable ">
      </el-switch>
    </div>
    <!--<span>It should be noted that the content will not be aligned in center by default</span>-->
    <div class="block">
      <!--<span class="demonstration">Switch when indicator is clicked</span>-->
      <el-carousel :autoplay="autoplay" trigger="click" height="80vh">
        <el-carousel-item v-for="(item, key, index) in items" :key="index">
          <h3 class="small text-center">{{ item.label }}</h3>
          <div class="my-flex-cont">
            <div class="my-flex-box">
              <img :src="item.imageSrc" class="gallery" alt="">
            </div>
          </div>
        </el-carousel-item>
      </el-carousel>
    </div>
    <!--<span slot="footer" class="dialog-footer">-->
    <!--<el-button type="primary" @click="dialogTableVisible = false" ><%= __('Close') %></el-button>-->
    <!--</span>-->
  </el-dialog>


