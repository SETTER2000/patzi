  <% /* start banner Area */ %>
  <section class="banner-area relative">
    <div class="container">
      <div class="row d-flex align-items-center justify-content-center">
        <div class="about-content col-lg-12">
          <h1 class="text-white"><%= __('Litter Details') %></h1>
          <p><%= __('Information about new arrivals to this world.') %></p>
        </div>
      </div>
    </div>
  </section>


  <% /* Portfolio Details Area */ %>
  <section class="portfolio_details_area section-gap">
    <div class="container">
      <div class="portfolio_details_inner">
        <div class="row">
          <div class="col-md-6">
            <div class="left_img">
              <a href="#" @click="showSlider()">
                <img class="img-fluid" :src="litter.imageSrc" alt="">
              </a>
            </div>
          </div>

          <div class="offset-md-1 col-md-5">
            <div class="portfolio_right_text mt-30">
              <h4><%= __('Litter') %> {{litter.letter.toUpperCase()}}</h4>
              <p v-if="litter.subtitle">{{litter.subtitle}}</p>
              <div class="block mb-5">

                <el-rate
                  v-model="ratio"
                  :colors="colors"
                  @change="changeRatio()"
                  show-score
                  text-color="#ff9900"
                  score-template=" <%= __('points') %> {value} "
                >
                </el-rate>
              </div>

              <ul class="list">
                <li><span><%= __('Dob') %>
                    <el-tooltip v-if="isAfterDate" class="item" effect="dark"
                                content="<%= __("Date of litter. If the date has not yet arrived, then the mating has taken place and the birth is expected on that date.") %>"
                                placement="right-start">
                           <i class="el-icon-question"></i>
                   </el-tooltip>
                  </span>: {{litter.born}}
                </li>
                <li><span><%= __('Name') %>
                    <el-tooltip class="item" effect="dark"
                                content="<%= __("The letter with which the puppy's name should begin.") %>"
                                placement="right-start">
                           <i class="el-icon-question"></i>
                   </el-tooltip>
                  </span>: {{litter.letter.toUpperCase()}}...
                </li>
                <li><span><%= __('Sire') %></span>: {{litter.sire}}</li>
                <li><span><%= __('Dam') %></span>: {{litter.dam}}</li>

              </ul>
            </div>
          </div>
